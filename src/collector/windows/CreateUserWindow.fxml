<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.RadioButton?>

<AnchorPane prefHeight="480.0" prefWidth="360" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="collector.windows.CreateUserWindow">
   <children>
   

      <Label layoutX="40.0" layoutY="30.0" text="Username:" />
      <TextField fx:id="username" layoutX="40.0" layoutY="50.0" prefHeight="25.0" prefWidth="280.0" promptText="Username" />
      
      <Label layoutX="40.0" layoutY="90.0" text="Password:" />
      <PasswordField fx:id="password" layoutX="40.0" layoutY="110.0" prefWidth="280.0" promptText="password" />
      
      <Separator layoutX="40.0" layoutY="148.0" prefWidth="280.0" />
      
      <Label layoutX="40.0" layoutY="170.0" text="Consumer Key:" />
      <TextField fx:id="consumerKey" layoutX="40.0" layoutY="190.0" prefWidth="280.0" promptText="Consumer Key" />
      
      <Label layoutX="40.0" layoutY="230.0" text="Consumer Secret:" />
      <TextField fx:id="consumerSecret" layoutX="40.0" layoutY="250.0" prefWidth="280.0" promptText="Consumer Secret" />
      
      <Label layoutX="40.0" layoutY="290.0" text="Access Token" />
      <TextField fx:id="accessToken" layoutX="40.0" layoutY="310.0" prefWidth="280.0" promptText="Access Token" />
      
      <Label layoutX="40.0" layoutY="350.0" text="Access Token Secret" />
      <TextField fx:id="accessTokenSecret" layoutX="40.0" layoutY="370.0" prefWidth="280.0" promptText="Access Token Secret" />
      
      <Button fx:id="createButton" onAction="#handleCreate" layoutX="60.0" layoutY="422.0" mnemonicParsing="false" prefWidth="60.0" text="Create" />
      <Button fx:id="cancelButton" onAction="#handleCancel" layoutX="150.0" layoutY="422.0" mnemonicParsing="false" prefWidth="60.0" text="Cancel" />
      <Button fx:id="helpButton" onAction="#handleHelp" layoutX="240.0" layoutY="422.0" mnemonicParsing="false" prefWidth="60.0" text="Help" />
   </children>
</AnchorPane>
